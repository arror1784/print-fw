{
  "main": "public/Main.js",
  "homepage": "./",
  "build": {
    "productName": "productTest",
    "appId": "kr.co.productTest.www",
    "asar": true,
    "protocols": {
      "name": "productTest",
      "schemes": [
        "productTest"
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./electron-root/favicon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "zip",
        "tar.gz"
      ],
      "icon": "./electron-root/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": false,
      "installerLanguages": [
        "en_US",
        "ko_KR"
      ],
      "language": "1042"
    },
    "directories": {
      "buildResources": "resources/installer/",
      "output": "build/",
      "app": "."
    }
  },
  "name": "print-fw",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^17.0.20",
    "@types/react-circular-progressbar": "^1.1.0",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "electron-is-dev": "^2.0.0",
    "global": "^4.4.0",
    "node-wifi": "^2.0.16",
    "react": "^17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react-start": "BROWSER=none yarn start",
    "react-start-win": "set BROWSER=none && yarn start",
    "electron-start": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron-start-win": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron-pack": "yarn build && electron-builder build -c.extraMetadata.main=build/Main.js",
    "build:osx": "build --mac",
    "build:linux": "npm run build:linux32 && npm run build:linux64",
    "build:linux32": "electron-builder build --linux --ia32",
    "build:linux64": "electron-builder build --linux --x64",
    "build:win": "npm run build:win32 && npm run build:win64",
    "build:win32": "electron-builder build --win --ia32",
    "build:win64": "electron-builder build --win --x64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "wait-on": "^6.0.1"
  }
}
